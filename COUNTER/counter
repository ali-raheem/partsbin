#! /usr/local/bin/vvp
:ivl_version "10.2 (stable)" "(v10_2)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x800942180 .scope module, "counter_tb" "counter_tb" 2 1;
 .timescale 0 0;
P_0x800952400 .param/l "WIDTH" 1 2 2, +C4<00000000000000000000000000001000>;
v0x800947460_0 .var "clk", 0 0;
v0x800947500_0 .net "out", 7 0, v0x800947280_0;  1 drivers
v0x8009475a0_0 .var "rst", 0 0;
v0x800947640_0 .var "updown", 0 0;
S_0x800942480 .scope module, "DUT" "counter" 2 8, 3 1 0, S_0x800942180;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "rst"
    .port_info 2 /INPUT 1 "updown"
    .port_info 3 /OUTPUT 8 "out"
P_0x800952480 .param/l "WIDTH" 0 3 2, +C4<00000000000000000000000000001000>;
v0x8009471e0_0 .net "clk", 0 0, v0x800947460_0;  1 drivers
v0x800947280_0 .var "out", 7 0;
v0x800947320_0 .net "rst", 0 0, v0x8009475a0_0;  1 drivers
v0x8009473c0_0 .net "updown", 0 0, v0x800947640_0;  1 drivers
E_0x800923120 .event posedge, v0x8009471e0_0;
    .scope S_0x800942480;
T_0 ;
    %wait E_0x800923120;
    %load/vec4 v0x800947320_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x800947280_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x8009473c0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x800947280_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x800947280_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x800947280_0;
    %subi 1, 0, 8;
    %assign/vec4 v0x800947280_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x800942180;
T_1 ;
    %vpi_call 2 16 "$dumpfile", "counter.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x800942180 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x800947460_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x800947640_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x8009475a0_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x8009475a0_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x800947460_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x8009475a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x800947460_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x800947640_0, 0, 1;
    %delay 150, 0;
    %vpi_call 2 31 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x800942180;
T_2 ;
    %delay 1, 0;
    %load/vec4 v0x800947460_0;
    %inv;
    %store/vec4 v0x800947460_0, 0, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter_tb.v";
    "counter.v";
