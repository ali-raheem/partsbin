:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x563698a12eb0 .scope module, "pwm_tb" "pwm_tb" 2 1;
 .timescale 0 0;
v0x563698a33700_0 .var "clk", 0 0;
v0x563698a337f0_0 .var "in", 3 0;
v0x563698a338c0_0 .net "out", 0 0, v0x563698a334b0_0;  1 drivers
v0x563698a339c0_0 .var "rst", 0 0;
S_0x563698a13030 .scope module, "DUT" "pwm" 2 7, 3 1 0, S_0x563698a12eb0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "rst"
    .port_info 2 /INPUT 4 "in"
    .port_info 3 /OUTPUT 1 "out"
P_0x563698a13200 .param/l "WIDTH" 0 3 2, +C4<00000000000000000000000000000100>;
v0x563698a132a0_0 .net "clk", 0 0, v0x563698a33700_0;  1 drivers
v0x563698a332e0_0 .var "counter", 3 0;
v0x563698a333c0_0 .net "in", 3 0, v0x563698a337f0_0;  1 drivers
v0x563698a334b0_0 .var "out", 0 0;
v0x563698a33570_0 .net "rst", 0 0, v0x563698a339c0_0;  1 drivers
E_0x5636989d6af0 .event posedge, v0x563698a132a0_0;
    .scope S_0x563698a13030;
T_0 ;
    %wait E_0x5636989d6af0;
    %load/vec4 v0x563698a33570_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x563698a332e0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x563698a332e0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x563698a332e0_0, 0;
    %load/vec4 v0x563698a332e0_0;
    %load/vec4 v0x563698a333c0_0;
    %cmp/u;
    %flag_or 5, 4;
    %jmp/0xz  T_0.2, 5;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x563698a334b0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x563698a334b0_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x563698a12eb0;
T_1 ;
    %vpi_call 2 14 "$dumpfile", "pwm_test.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x563698a13030 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563698a33700_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x563698a337f0_0, 0, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x563698a339c0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563698a339c0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x563698a337f0_0, 0, 4;
    %delay 50000, 0;
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x563698a12eb0;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x563698a33700_0;
    %inv;
    %store/vec4 v0x563698a33700_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x563698a12eb0;
T_3 ;
    %delay 100, 0;
    %load/vec4 v0x563698a337f0_0;
    %addi 1, 0, 4;
    %store/vec4 v0x563698a337f0_0, 0, 4;
    %jmp T_3;
    .thread T_3;
# The file index is used to find the file name in the following table.
